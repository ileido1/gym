<%= form_for (@client) do |f| %>
<% @client.errors.full_messages.each do |message| %>
<div class="">
  <%= message %>
<% end %>
  </div>
  <div class="">
    Foto del cliente: <%= f.file_field :foto %>
  </div>
  <div class="">
    <%= f.text_field :nombre, placeholder: "Nombre" %>
  </div>
  <div class="">
    <%= f.text_field :apellido, placeholder: "Apellido" %>
  </div>
  <div class="">
    <%= f.text_field :cedula, placeholder: "Cedula" %>
  </div>
  <div class="">
    <%= f.email_field :email, placeholder: "Email" %>
  </div>
  <div class="">
    <%= f.telephone_field :telefono, placeholder: "Telefono" %>
  </div>
  <div class="">
    <%= f.label :Fecha_de_nacimiento%>
    <%= f.date_field :nacimiento %>
  </div>
  <div class="">
    <%= f.text_field :direccion, placeholder: "Direccion" %>
  </div>
  <div class="">
    <%= f.submit "Guardar" %>
  </div>
<% end %>
