<div class="container-fluid">
<div  class="row", id="resultados">

</div>
</div>

<div class="container-fluid">
  <div class="row">
    <div class="col-sm-12 col-md-12 col-xs-12 anchura">
      <div class="container-fluid">

        <%= form_tag "/search/create", method: :get, remote: true, id: "search_f" do %>
          <%= text_field_tag(:search,params[:search],placeholder: "Buscador de clientes", class: "form-control form-control-1"  ) %>
            <%= content_tag :button, type: :submit, class: "btn btn-default" do %>
              Buscar
            <% end %>
          </div>
    </div>
    </div>
    <% end %>



<div class="container-fluid">
    <div class="row">
    <%= render @clients %>
  </div>
  <nav aria-label="...">
    <ul class="pager">
    <li> <%= link_to_prev_page @clients, 'Anterior' %></li>
    <li> <%= link_to_next_page @clients, 'Siguiente' %></li>
  </ul>
</nav>
</div>
